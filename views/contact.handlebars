<section class="card page-card">
  <h1>Contact</h1>
  <p>Want features like moods, tags, search, or password lock? Send a note.</p>

  <form class="contact-form" onsubmit="event.preventDefault(); alert('Submitted (demo).');">
    <label>
      Name
      <input type="text" required />
    </label>

    <label>
      Email
      <input type="email" required />
    </label>

    <label>
      Message
      <textarea rows="6" required></textarea>
    </label>

    <button class="btn primary" type="submit">Send</button>
  </form>
</section>
